"UnlitGeneric"
{
	"$baseTexture" "models/mechassault_2/mechs/cougar/mech_color_reactor"
	"$translucent"	"1"
	"$additive"	"0"
	"$blendtintbybasealpha" 1

	Proxies
	{
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}


}

