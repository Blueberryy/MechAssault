VertexLitGeneric
{
	$baseTexture		"models\mechassault_2\mechs\elemental\elemental"
	$bumpmap 		"models\mechassault_2\mechs\elemental\elemental_normal"
	$envmap			env_cubemap
	$normalmapalphaenvmapmask		1
	$envmaptint "[0.6 0.6 0.6]"
	$ambientocclusion	1
	$halflambert		1
	$diffuseexp			1.5
	$nocull		1
	$rimlight               1
	$rimlightexponent       30
	$rimlightboost          1	
	$emissiveBlendEnabled 1
	$emissiveBlendTexture     "models\mechassault_2\mechs\elemental\elemental"
	$emissiveBlendBaseTexture	"models\mechassault_2\mechs\elemental\elemental_illum"
	$emissiveBlendFlowTexture     "vgui\white"
	$emissiveBlendTint "[1 1 1 ]"
	$emissiveBlendStrength 10
	$emissiveBlendScrollVector "[0 0]"

"$blendtintbybasealpha" 1

	Proxies
	{
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.5
			max 0.8
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
		Clamp
		{
			min 0.01
			max 5
			srcVar1 "$color"
			resultVar "$emissiveBlendTint"
		}
	}


}