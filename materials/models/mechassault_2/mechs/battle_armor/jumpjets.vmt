"UnlitGeneric"
{
	"$basetexture"	"models\mechassault\lonewolf\battle_armor\jumpjets"
	"$additive"	1
	"$vertexcolor"	1
	"$vertexalpha"	1

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "30"
      }
   }

}