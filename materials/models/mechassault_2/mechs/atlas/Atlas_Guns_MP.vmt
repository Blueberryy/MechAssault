"VertexlitGeneric"
{
	"$basetexture" "models/mechassault_2/mechs/atlas/Atl_Arms_low"
	"$bumpmap" "models/mechassault_2/mechs/atlas/atlas_guns_norm"
	$normalmapalphaenvmapmask 		1
	$envmap			env_cubemap
	$envmaptint "[0.12 0.12 0.12]"
	$envmapfresnel 0.3
	"$model" 1
	"$phong" 1
	"$phongboost" .05
	"$phongexponent" 25
	"$phongtint"	"[.5 .5 .5]"
	"$rimlight" 1
	"$rimlightexponent"       11
	"$phongfresnelranges" "[1.2 3 8]"
	"$nocull"		1
	"$blendtintbybasealpha"	"1"

	Proxies
	{
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.8
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}
}




