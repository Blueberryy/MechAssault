	$normalmapalphaenvmapmask 		1
	$envmap			env_cubemap
	$envmaptint "[0.25 0.25 0.25]"
	"$model" 1
	"$phong" 1
	"$phongboost" 0.65
	"$phongexponent" 15
	"$phongtint"	"[.5 .5 .5]"
	"$rimlight" 1
	"$rimlightexponent"       11
	"$phongfresnelranges" "[1.2 3 8]"
	"$blendtintbybasealpha" 1

	Proxies
	{
		Clamp
		{
			min 0.45
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 0.7
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
		Clamp
		{
			min 0
			max 1
			srcVar1 "$color"
			resultVar "$emissiveBlendTint"
