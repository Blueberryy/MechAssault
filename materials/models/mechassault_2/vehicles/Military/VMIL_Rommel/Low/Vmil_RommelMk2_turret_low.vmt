"VertexlitGeneric"
{
	"$basetexture" "models/mechassault_2/vehicles/Military/VMIL_Rommel/Vmil_RommelMk2_turret_Low"
	"$bumpmap" "models/mechassault_2/vehicles/Military/VMIL_Rommel/Vmil_RommelMk2_turret_lownorm"
	"$model" 1
	"$phong" 1
	"$phongboost" .15
	"$phongexponent" 5
	"$rimlight" 1
	"$rimlightexponent"       11
	"$phongfresnelranges" "[1.2 3 8]"

	"$blendtintbybasealpha"	"1"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}
}




