"UnlitGeneric"
{
	"$basetexture"	"effects/mechassault2/extra/explosion_generic"
	"$color2"	"[2 2 2]"
	"$additive"	1
	"$vertexcolor"	1
	"$vertexalpha"	1


"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "15"
      }
   }

}